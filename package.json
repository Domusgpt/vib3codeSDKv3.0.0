{
  "name": "@vib3/sdk",
  "version": "1.9.0",
  "description": "VIB3+ 4D Visualization SDK - Unified engine with 6D rotation, MCP agentic integration, and cross-platform support",
  "type": "module",
  "main": "src/core/VIB3Engine.js",
  "types": "./types/adaptive-sdk.d.ts",
  "bin": {
    "vib3": "./src/cli/index.js"
  },
  "sideEffects": [
    "./src/geometry/builders/*.js"
  ],
  "exports": {
    ".": {
      "types": "./types/adaptive-sdk.d.ts",
      "import": "./src/core/VIB3Engine.js"
    },
    "./core": {
      "import": "./src/core/VIB3Engine.js"
    },
    "./engine": {
      "import": "./src/core/VIB3Engine.js"
    },
    "./parameters": {
      "import": "./src/core/Parameters.js"
    },
    "./geometry": {
      "import": "./src/geometry/index.js"
    },
    "./geometry/factory": {
      "import": "./src/geometry/GeometryFactory.js"
    },
    "./geometry/generators": {
      "import": "./src/geometry/generators/Tesseract.js"
    },
    "./geometry/generators/tesseract": {
      "import": "./src/geometry/generators/Tesseract.js"
    },
    "./geometry/generators/tetrahedron": {
      "import": "./src/geometry/generators/Tetrahedron.js"
    },
    "./geometry/generators/sphere": {
      "import": "./src/geometry/generators/Sphere.js"
    },
    "./geometry/generators/torus": {
      "import": "./src/geometry/generators/Torus.js"
    },
    "./geometry/generators/klein-bottle": {
      "import": "./src/geometry/generators/KleinBottle.js"
    },
    "./geometry/generators/fractal": {
      "import": "./src/geometry/generators/Fractal.js"
    },
    "./geometry/generators/wave": {
      "import": "./src/geometry/generators/Wave.js"
    },
    "./geometry/generators/crystal": {
      "import": "./src/geometry/generators/Crystal.js"
    },
    "./geometry/warp": {
      "import": "./src/geometry/warp/index.js"
    },
    "./geometry/warp/hypersphere": {
      "import": "./src/geometry/warp/HypersphereCore.js"
    },
    "./geometry/warp/hypertetra": {
      "import": "./src/geometry/warp/HypertetraCore.js"
    },
    "./geometry/buffers": {
      "import": "./src/geometry/buffers/index.js"
    },
    "./scene": {
      "import": "./src/scene/index.js"
    },
    "./scene/node4d": {
      "import": "./src/scene/Node4D.js"
    },
    "./scene/scene4d": {
      "import": "./src/scene/Scene4D.js"
    },
    "./scene/resources": {
      "import": "./src/scene/ResourceManager.js"
    },
    "./scene/pools": {
      "import": "./src/scene/MemoryPool.js"
    },
    "./scene/disposable": {
      "import": "./src/scene/Disposable.js"
    },
    "./render": {
      "import": "./src/render/index.js"
    },
    "./render/state": {
      "import": "./src/render/RenderState.js"
    },
    "./render/commands": {
      "import": "./src/render/RenderCommand.js"
    },
    "./render/buffer": {
      "import": "./src/render/CommandBuffer.js"
    },
    "./render/target": {
      "import": "./src/render/RenderTarget.js"
    },
    "./render/shader": {
      "import": "./src/render/ShaderProgram.js"
    },
    "./render/webgl": {
      "import": "./src/render/backends/WebGLBackend.js"
    },
    "./wasm": {
      "import": "./src/wasm/index.js"
    },
    "./wasm/loader": {
      "import": "./src/wasm/WasmLoader.js"
    },
    "./types/render": {
      "types": "./types/render/index.d.ts"
    },
    "./quantum": {
      "import": "./src/quantum/QuantumEngine.js"
    },
    "./faceted": {
      "import": "./src/faceted/FacetedSystem.js"
    },
    "./holographic": {
      "import": "./src/holograms/RealHolographicSystem.js"
    },
    "./agent": {
      "import": "./src/agent/index.js"
    },
    "./agent/telemetry": {
      "import": "./src/agent/telemetry/index.js"
    },
    "./agent/mcp": {
      "import": "./src/agent/mcp/index.js"
    },
    "./agent/cli": {
      "import": "./src/agent/cli/index.js"
    },
    "./agent/exporters": {
      "import": "./src/agent/telemetry/TelemetryExporters.js"
    },
    "./agent/streaming": {
      "import": "./src/agent/telemetry/EventStream.js"
    },
    "./agent/instrumentation": {
      "import": "./src/agent/telemetry/Instrumentation.js"
    },
    "./schemas": {
      "import": "./src/schemas/index.js"
    },
    "./schemas/parameters": {
      "import": "./src/schemas/parameters.schema.json",
      "require": "./src/schemas/parameters.schema.json"
    },
    "./schemas/tool-response": {
      "import": "./src/schemas/tool-response.schema.json",
      "require": "./src/schemas/tool-response.schema.json"
    },
    "./schemas/error": {
      "import": "./src/schemas/error.schema.json",
      "require": "./src/schemas/error.schema.json"
    },
    "./cli": {
      "node": "./src/cli/index.js"
    },
    "./gallery": {
      "import": "./src/gallery/GallerySystem.js"
    },
    "./viewer": {
      "import": "./src/viewer/index.js"
    },
    "./viewer/portal": {
      "import": "./src/viewer/ViewerPortal.js"
    },
    "./viewer/reactivity": {
      "import": "./src/viewer/ReactivityManager.js"
    },
    "./viewer/card-bending": {
      "import": "./src/viewer/CardBending.js"
    },
    "./viewer/card-exporter": {
      "import": "./src/viewer/TradingCardExporter.js"
    },
    "./viewer/audio": {
      "import": "./src/viewer/AudioReactivity.js"
    },
    "./viewer/gallery-ui": {
      "import": "./src/viewer/GalleryUI.js"
    },
    "./llm": {
      "import": "./src/llm/LLMParameterInterface.js"
    },
    "./math": {
      "import": "./src/math/index.js"
    },
    "./math/vec4": {
      "import": "./src/math/Vec4.js"
    },
    "./math/rotor4d": {
      "import": "./src/math/Rotor4D.js"
    },
    "./math/mat4x4": {
      "import": "./src/math/Mat4x4.js"
    },
    "./math/projection": {
      "import": "./src/math/Projection.js"
    },
    "./math/constants": {
      "import": "./src/math/constants.js"
    },
    "./quaternion": "./src/ui/adaptive/renderers/ShaderQuaternionSynchronizer.js",
    "./sensors": "./src/ui/adaptive/SensoryInputBridge.js",
    "./polychora": "./src/core/PolychoraSystem.js",
    "./core/quaternion": {
      "types": "./src/core/quaternion/index.ts",
      "default": "./src/core/quaternion/index.ts"
    },
    "./core/quaternion/poseSchema": {
      "types": "./src/core/quaternion/poseSchema.ts",
      "default": "./src/core/quaternion/poseSchema.ts"
    },
    "./core/quaternion/registry": {
      "types": "./src/core/quaternion/registry.ts",
      "default": "./src/core/quaternion/registry.ts"
    },
    "./ui/adaptive/renderers/webgpu": {
      "types": "./src/ui/adaptive/renderers/webgpu/index.ts",
      "default": "./src/ui/adaptive/renderers/webgpu/index.ts"
    },
    "./ui/adaptive/renderers/pose-registry": {
      "types": "./src/ui/adaptive/renderers/QuaternionPoseRegistrySynchronizer.ts",
      "default": "./src/ui/adaptive/renderers/QuaternionPoseRegistrySynchronizer.ts"
    },
    "./ui/adaptive/renderers/pose-monitor": {
      "types": "./src/ui/adaptive/renderers/PoseReliabilityMonitor.ts",
      "default": "./src/ui/adaptive/renderers/PoseReliabilityMonitor.ts"
    },
    "./ui/adaptive/renderers/pose-confidence": {
      "types": "./src/ui/adaptive/renderers/poseConfidence.ts",
      "default": "./src/ui/adaptive/renderers/poseConfidence.ts"
    },
    "./ui/adaptive/localization": {
      "types": "./src/ui/adaptive/localization/index.ts",
      "default": "./src/ui/adaptive/localization/index.ts"
    },
    "./product/telemetry/facade": {
      "types": "./types/product/telemetry/createTelemetryFacade.d.ts",
      "default": "./src/product/telemetry/createTelemetryFacade.js"
    },
    "./product/telemetry/reference-providers": {
      "types": "./types/product/telemetry/referenceTelemetryProviders.d.ts",
      "default": "./src/product/telemetry/providers/referenceTelemetryProviders.js"
    }
  },
  "keywords": [
    "quaternion",
    "4d",
    "xr",
    "openxr",
    "webxr",
    "spatial-computing",
    "polytope",
    "tesseract",
    "geometric-processing",
    "ar",
    "vr",
    "mixed-reality"
  ],
  "author": {
    "name": "Paul Phillips",
    "email": "Paul@clearseassolutions.com",
    "url": "https://parserator.com"
  },
  "license": "SEE LICENSE IN DOCS/LICENSE_ATTESTATION_PROFILE_CATALOG.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/Domusgpt/vib34d-xr-quaternion-sdk.git"
  },
  "engines": {
    "node": ">=18.19.0"
  },
  "scripts": {
    "dev:web": "vite --open",
    "build:web": "vite build",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "test": "vitest run",
    "bench": "vitest bench",
    "analyze:bundle": "vite build --mode analyze",
    "check:bundle": "pnpm exec vite build --outDir dist-ci --emptyOutDir && node tools/ci/bundleSizeCheck.mjs dist-ci --cleanup",
    "codegen:localization": "tsx tools/codegen/localization-quaternions.ts",
    "cli:telemetry": "node tools/cli/agent-cli.js telemetry:export",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "validate:phase5-metrics": "node tools/telemetry/validatePhase5Metrics.mjs"
  },
  "packageManager": "pnpm@9.4.0",
  "files": [
    "src/",
    "types/",
    "DOCS/",
    "README.md"
  ],
  "dependencies": {
    "ajv": "^8.17.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@vitest/coverage-v8": "^1.6.1",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/html-vite": "^8.1.6",
    "eslint": "^9.6.0",
    "storybook": "^8.1.6",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vitest": "^1.6.0",
    "tsx": "^4.15.7"
  }
}
